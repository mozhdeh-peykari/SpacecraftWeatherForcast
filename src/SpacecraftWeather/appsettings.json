{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=SpacecraftWeather;Trusted_Connection=True;TrustServerCertificate=true"
  },
  "WeatherWebServiceSettings": {
    "Endpoint": "https://api.open-meteo.com/v1/",
    "Resource": "forecast",
    "WeatherWebServiceParameters": [
      {
        "ParameterType": "latitude",
        "ParameterName": "latitude"
      },
      {
        "ParameterType": "longitude",
        "ParameterName": "longitude"
      },
      {
        "ParameterType": "miscellaneous",
        "ParameterName": "hourly",
        "ParameterValue": "temperature_2m"
      }
    ]
  }
}
